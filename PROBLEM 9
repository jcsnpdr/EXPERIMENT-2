#include <iostream>
#include <iomanip>
using namespace std;

int main()
{
    int i,j,r,c;
    
  cout << "how many rows: ";
  cin >> r;
  cout << "how many column: ";
  cin >> c;
  
    for(i=1; i<=r; i++)
    {
        for(j=1; j<=c; j++)
        {
            cout << "*";     
        }
         cout <<endl;   
    }
    return 0;
}
